{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "scripts": {
    "clean": "rescript clean -with-deps",
    "gen-gql-types": "rm -f graphql-schema.json && /bin/bash ./code-gen/generate-schema-json",
    "dev": "next",
    "debug": "NODE_OPTIONS='--inspect' next",
    "build": "rescript && next build",
    "now-build": "rescript && next build",
    "export": "next export",
    "start": "next start -p $PORT",
    "res:build": "rescript",
    "res:clean": "rescript clean",
    "res:start": "rescript build -w"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.5.4",
    "next": "10.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@reasonml-community/graphql-ppx": "^1.2.2",
    "@rescript/react": "^0.10.3",
    "next-transpile-modules": "7.1.2",
    "gentype": "4.1",
    "autoprefixer": "10.1.0",
    "rescript": "*",
    "cssnano": "5.0.5",
    "postcss": "8.2.15",
    "postcss-cli": "8.3.1",
    "tailwindcss": "2.0.4",
    "rescript-apollo-client": "^2.2.0",
    "rescript-webapi": "^0.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
